using System;
public class PrintWorld
{
public static void Main()
{
int i =1;
int n;
Console.WriteLine("Enter A Number");
n = Convert.ToInt32(Console.ReadLine());
Console.WriteLine("table of given number");
while (i <= 10)
{
Console.WriteLine( n * i);
i++;
}
Console.ReadLine();
}
}
Output:-
Enter A Number
3
table of given number
3
6
9
12
15
